{
  "auto-close-tag.SublimeText3Mode": true,
  "breadcrumbs.enabled": true,
  "cSpell.language": "en,de",
  "css.lint.boxModel": "warning",
  "css.lint.compatibleVendorPrefixes": "warning",
  "css.lint.duplicateProperties": "warning",
  "css.lint.emptyRules": "ignore",
  "css.lint.float": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.ieHack": "warning",
  "css.lint.importStatement": "warning",
  "css.lint.important": "warning",
  "css.lint.universalSelector": "warning",
  "css.lint.unknownVendorSpecificProperties": "warning",
  "css.lint.zeroUnits": "warning",
  "css.trace.server": "verbose",
  "debug.allowBreakpointsEverywhere": true,
  "debug.inlineValues": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorStyle": "line",
  "editor.detectIndentation": true,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.lineHeight": 22,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "editor.rulers": [80, 120],
  "editor.tabCompletion": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "eslint._legacyModuleResolve": true,
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.provideLintTask": true,
  "eslint.trace.server": "verbose",
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.css": "postcss",
    "*.json": "jsonc",
    ".eslintrc": "jsonc"
  },
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "javascript",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "gitHistory.hideCommitViewExplorer": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.codeLens.scopes": ["document", "containers"],
  "gitlens.historyExplorer.enabled": false,
  "gitlens.insiders": true,
  "gitlens.outputLevel": "errors",
  "gitlens.settings.mode": "advanced",
  "html.format.endWithNewline": true,
  "html.format.wrapAttributes": "force-aligned",
  "html.suggest.ionic": true,
  "html.trace.server": "verbose",
  "instantmarkdown.autoCloseServerAndBrowser": false,
  "instantmarkdown.autoOpenBrowser": false,
  "jshint.options": {
    "esversion": 6
  },
  "markdown-preview-enhanced.enableCriticMarkupSyntax": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableScriptExecution": true,
  "markdown.preview.scrollPreviewWithEditor": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",
  "scm.alwaysShowProviders": true,
  "sortJSON.orderOverride": ["name", "version", "description"],
  "sortJSON.orderUnderride": ["dependencies", "devDependencies"],
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.2,
  "tsimporter.filesToScan": ["**/*.ts", "**/*.tsx", "**/*.vue"],
  "typescript.navigator.showDataTypes": true,
  "typescript.navigator.showIcons": true,
  "typescript.navigator.showVisibilityLabels": true,
  "typescript.referencesCodeLens.enabled": true,
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.postcss": "prettier",
  "vetur.format.defaultFormatter.scss": "prettier",
  "vetur.format.scriptInitialIndent": false,
  "vetur.format.styleInitialIndent": false,
  "vsicons.dontShowNewVersionMessage": true,
  "window.zoomLevel": 1.12,
  "workbench.colorTheme": "Cobalt2",
  "workbench.editor.swipeToNavigate": true,
  "workbench.iconTheme": "vscode-great-icons",
  "workbench.tree.horizontalScrolling": true
}