{
  "window.zoomLevel": 0.2,
  "terminal.integrated.fontSize": 13,
  "workbench.iconTheme": "material-icon-theme",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.arrowParens": "always",
  "prettier.printWidth": 70,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "typescript.preferences.quoteStyle": "single",
  "prettier.jsxSingleQuote": true,
  "javascript.preferences.quoteStyle": "single",
  "editor.minimap.enabled": false,
  "workbench.colorTheme": "Slack Theme Aubergine Dark",
  "workbench.colorCustomizations": {
    "activityBar.foreground": "#feabea",
    "statusBar.background": "#430133",
    "editorGroup.border": "#feabea44",
    "titleBar.activeBackground": "#000",
    "editorLineNumber.foreground": "#666",
    "editorIndentGuide.background": "#66666666"
  },

  "editor.codeLens": false,
  "editor.fontSize": 13,
  "editor.wordWrapColumn": 70,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.suggest.localityBonus": true,
  "workbench.list.horizontalScrolling": true,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
